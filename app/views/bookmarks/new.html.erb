<h1>New Bookmark</h1>

<%= form_for :bookmark, url: bookmarks_path do |f| %>
<p>
  <%= f.label :title %>
  <%= f.text_field :title %>
</p>

<p>
  <%= f.label :url %>
  <%= f.text_field :url %>
</p>

<p>
  <%= f.label :description %>
  <%= f.text_area :description %>
</p>

<p>
  <%= f.submit %>
</p>
<% end %>
